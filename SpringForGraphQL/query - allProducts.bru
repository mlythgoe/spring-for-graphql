meta {
  name: query - allProducts
  type: graphql
  seq: 1
}

post {
  url: http://localhost:8080/graphql
  body: graphql
  auth: inherit
}

body:graphql {
  query {
      allProducts{
          id
          desc
          title
          price
          productPriceHistories {
              id
              startDate
              price
          }
  
      }
  }
}

settings {
  encodeUrl: true
}
