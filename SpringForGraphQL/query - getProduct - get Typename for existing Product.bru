meta {
  name: query - getProduct - get Typename for existing Product
  type: graphql
  seq: 4
}

post {
  url: http://localhost:8080/graphql
  body: graphql
  auth: inherit
}

body:graphql {
  query {
    getProduct(id:"01994ebf-796c-74eb-b603-b24c5cc1e265"){
      __typename
      id
      desc
      title
      price
      productPriceHistories {
        __typename
        id
        startDate
        price
      }
    }
  }
}

settings {
  encodeUrl: true
  timeout: 0
}
